@model Tuple<List<IJERTS.Objects.Paper>, List<IJERTS.Objects.Users>, List<IJERTS.Objects.Queries>> 
@{
    ViewData["Title"] = "Dashboard";
}

<div class="row">

    <div class="row">
        <div class="col-sm-12">
            <h2>Dashboard</h2>
        </div>
    </div>

    <div class="col-xl-12">
        <div class="card-box">

            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a href="#ManagePapers" data-toggle="tab" aria-expanded="false" class="nav-link active">
                        <i class="fi-monitor mr-2"></i>Manage Papers
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#ManageReviewer" data-toggle="tab" aria-expanded="true" class="nav-link">
                        <i class="fi-head mr-2"></i>Manage Reviewer
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#ManageQueries" data-toggle="tab" aria-expanded="false" class="nav-link">
                        <i class="fi-mail mr-2"></i> Manage Queries
                    </a>
                </li>
            </ul>
            <div class="tab-content">
                <div class="tab-pane show active" id="ManagePapers">
                    @Html.Partial("_ManagePaper", Model.Item1)
                </div>
                <div class="tab-pane" id="ManageReviewer">
                    @Html.Partial("_ManageReviewer", Model.Item2)
                </div>
                <div class="tab-pane" id="ManageQueries">
                    @Html.Partial("_ManageQueries", Model.Item3)
                </div>
            </div>

        </div>
    </div>
</div>